@top AlertRuleSearch { expression+ }

expression { FilterExpression expression }

FilterExpression { FilterType ":" FilterValue }

FilterType { "ns" | "group" | "rule" | "ds" | "state" | "type" | "label" }

@skip { space }

@tokens {
  space { @whitespace+ }
  FilterValue { $[a-zA-Z0-9,./?=!@#$%^&*()+-]+ }
}
